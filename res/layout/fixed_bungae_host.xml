<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#D8DDE0" >

    

    <ScrollView
        android:id="@+id/fhscrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/adview" >

        <LinearLayout
            android:id="@+id/fhlinearLayout6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
            
            <TextView
        android:id="@+id/fhtextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:text="기본정보"
        android:textColor="#000000"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TableLayout
        android:id="@+id/fhsectionLayout1"
        android:layout_width="match_parent"
        android:layout_height="204dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_gravity="center"
        android:background="@drawable/rounded_back" >

        <TableRow
            android:id="@+id/fhtableRow1"
            android:layout_width="wrap_content"
            android:layout_height="50dp">

            <LinearLayout
                android:id="@+id/fhlinearLayout1"
                android:layout_width="wrap_content"
                android:layout_height="50dp" >

                <ImageView
                android:id="@+id/fhimageView3"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center"
                android:src="@drawable/id" />
                
                <TextView
                    android:id="@+id/fhCategoryTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="회원   I D "
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <View
            		android:id="@+id/fhostline1"
            		android:layout_width="1dp"
            		android:layout_height="50dp"
            		android:layout_marginLeft="5.5dp"
            		android:layout_marginRight="5dp"
            		android:background="#AAAAAA" />

                <TextView
                    android:id="@+id/fhcategoryLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text=""
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

            </LinearLayout>

        </TableRow>

        <View
            android:id="@+id/fhview1"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="#AAAAAA" />

        <TableRow
            android:id="@+id/fhtableRow12"
            android:layout_width="wrap_content"
            android:layout_height="50dp">
            
            <LinearLayout
                android:id="@+id/fhlinearLayout12"
                android:layout_width="wrap_content"
                android:layout_height="50dp" >

                <ImageView
                android:id="@+id/fhimageView32"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center"
                android:src="@drawable/phone" />
                
                <TextView
                    android:id="@+id/fhPhoneTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="전화 번호"
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <View
            		android:id="@+id/fhostline2"
            		android:layout_width="1dp"
            		android:layout_height="50dp"
            		android:layout_marginLeft="6dp"
            		android:layout_marginRight="5dp"
            		android:background="#AAAAAA" />

                <TextView
                    android:id="@+id/fhphoneLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text=""
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

            </LinearLayout>
            
            
        </TableRow>
        
        <View
            android:id="@+id/fhview12"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="#AAAAAA" />    
            
        
        <TableRow
            android:id="@+id/fhtableRow2"
            android:layout_width="wrap_content"
            android:layout_height="50dp" >
            
            <LinearLayout
                android:id="@+id/fhlinearLayout2"
                android:layout_width="wrap_content"
                android:layout_height="50dp" >

                <ImageView
                android:id="@+id/fhimageView3"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center"
                android:src="@drawable/toilet" />
                
                <TextView
                    android:id="@+id/fhTitleTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="회원 성별"
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <View
            		android:id="@+id/fhostline3"
            		android:layout_width="1dp"
            		android:layout_height="50dp"
            		android:layout_marginLeft="6dp"
            		android:layout_marginRight="5dp"
            		android:background="#AAAAAA" />


                <TextView
                    android:id="@+id/fhtitleLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text=""
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

            </LinearLayout>
            
        </TableRow>

        <View
            android:id="@+id/fhview2"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="#AAAAAA" />
            
        <TableRow
            android:id="@+id/fhtableRow3"
            android:layout_width="wrap_content"
            android:layout_height="50dp" >
            
            <LinearLayout
                android:id="@+id/fhlinearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="50dp" >

                <ImageView
                android:id="@+id/fhimageView3"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center"
                android:src="@drawable/age" />
                
                <TextView
                    android:id="@+id/fhHostTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="회원 나이"
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <View
            		android:id="@+id/fhostline4"
            		android:layout_width="1dp"
            		android:layout_height="50dp"
            		android:layout_marginLeft="6dp"
            		android:layout_marginRight="5dp"
            		android:background="#AAAAAA" />


                <TextView
                    android:id="@+id/fhhostLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text=""
                    android:textColor="#000000"
                    android:layout_gravity="center"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

            </LinearLayout>
            
        </TableRow>
        
        

    </TableLayout>

    <TextView
        android:id="@+id/fhtextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="자기 소개"
        android:textColor="#000000"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />


    <TableLayout
        android:id="@+id/fhsectionLayout2"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_gravity="center"
        android:background="@drawable/rounded_back"
        android:padding="5dp" >

        <TableRow
            android:id="@+id/fhtableRow6"
            android:layout_width="wrap_content"
            android:layout_height="160dp" >

            <TextView
                android:id="@+id/fhcontentText"
                android:layout_width="wrap_content"
                android:layout_height="160dp"
                android:layout_weight="1"
                android:padding="5dp"
                android:text=""
                android:textColor="#000000"
                android:textAppearance="?android:attr/textAppearanceMedium" />

        </TableRow>

    </TableLayout>

    
    
        </LinearLayout>
    </ScrollView>
    
	<net.daum.mobilead.MobileAdView
    	android:id="@+id/adview"
    	android:visibility="invisible" 
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_alignParentBottom="true"/>

</RelativeLayout>