<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#D8DDE0" >

    
        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
        		android:paddingBottom="20dp" >
            
            <TextView
        		android:id="@+id/SettingTitle1"
        		android:layout_width="wrap_content"
        		android:layout_height="wrap_content"
        		android:text="공지사항"
        		android:textColor="#000000"
        		android:layout_marginLeft="5dp"
        		android:layout_marginTop="20dp"
        		android:layout_marginBottom="5dp"
        		android:textAppearance="?android:attr/textAppearanceMedium" />


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_gravity="center" >

        <Button
            android:id="@+id/setting1Button"
            android:layout_width="match_parent"
        	android:layout_height="50dp"
        	android:layout_marginLeft="5dp"
        	android:layout_marginRight="5dp"
            android:text=""
            android:background="#00000000" />
        
        <TableLayout
        android:id="@+id/setting_sectionLayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/rounded_back"
        android:padding="5dp" >

        <TableRow
            android:id="@+id/setting_tablerow1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="center" >

            <RelativeLayout
        		android:id="@+id/settings_relativeLayout1"
        		android:layout_width="wrap_content"
        		android:layout_height="50dp"
        		android:layout_gravity="center"
        		android:gravity="center" >
            
            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:layout_centerVertical="true"
                android:src="@drawable/board" />

            <TextView
                android:id="@+id/setting_noticeLabel"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="15dp"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:text="공지사항"
                android:textColor="#000000"
                android:layout_toRightOf="@id/imageView1"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <LinearLayout
                    android:id="@+id/setting_linearLayout1"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_toRightOf="@id/setting_noticeLabel"
                    android:gravity="right"
                    android:layout_weight="1" >

            	<ImageView
               		android:id="@+id/arrowimageView1"
                	android:layout_width="40dp"
                	android:layout_height="40dp"
                	android:layout_gravity="center"
                	android:layout_marginRight="10dp"
                	android:src="@drawable/arrow_right" />

            </LinearLayout>
            
            </RelativeLayout>
        </TableRow>
    </TableLayout>
        
    </RelativeLayout>
        
    <TextView
        android:id="@+id/SettingTitle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용자정보"
        android:textColor="#000000"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
        
        
    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_gravity="center" >
        
        <Button
            android:id="@+id/setting2Button"
            android:layout_width="match_parent"
        	android:layout_height="50dp"
        	android:layout_marginLeft="5dp"
        	android:layout_marginRight="5dp"
            android:text=""
            android:background="#00000000" />
        
        <TableLayout
        android:id="@+id/setting_sectionLayout2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_gravity="center"
        android:background="@drawable/rounded_back"
        android:padding="5dp" >


        <TableRow
            android:id="@+id/setting_tablerow2"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="40dp" >

            <RelativeLayout
        		android:id="@+id/settings_relativeLayout2"
        		android:layout_width="wrap_content"
        		android:layout_height="50dp"
        		android:layout_gravity="center"
        		android:gravity="center" >
            
            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:src="@drawable/contact"
                android:layout_centerVertical="true" />
            
            <TextView
                android:id="@+id/setting_accountLabel"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="15dp"
                android:text="계정정보"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/imageView2"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <LinearLayout
                    android:id="@+id/setting_linearLayout2"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_toRightOf="@id/setting_noticeLabel"
                    android:gravity="right"
                    android:layout_weight="1" >
            
            <ImageView
                android:id="@+id/arrowimageView2"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:src="@drawable/arrow_right" />
            
            </LinearLayout>
            
            </RelativeLayout>
                
        </TableRow>

    </TableLayout>
        
    </RelativeLayout>
    
    
    
    
        

    <TextView
        android:id="@+id/SettingTitle3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="푸시"
        android:textColor="#000000"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <TableLayout
        android:id="@+id/setting_sectionLayout3"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_gravity="center"
        android:background="@drawable/rounded_back"
        android:padding="5dp" >


        <TableRow
            android:id="@+id/setting_tablerow3"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="40dp" >

            <RelativeLayout
        		android:id="@+id/settings_relativeLayout3"
        		android:layout_width="wrap_content"
        		android:layout_height="50dp"
        		android:layout_gravity="center"
        		android:gravity="center" >
            
            	<ImageView
                	android:id="@+id/imageView3"
                	android:layout_width="40dp"
                	android:layout_height="40dp"
                	android:layout_marginLeft="5dp"
                	android:gravity="center"
                	android:layout_centerVertical="true"
                	android:src="@drawable/warning" />
            
            	<TextView
                	android:id="@+id/setting_pushLabel"
                	android:layout_width="wrap_content"
                	android:layout_height="40dp"
                	android:layout_marginLeft="15dp"
                	android:text="푸시알림"
                	android:textColor="#000000"
                	android:gravity="center"
                	android:layout_centerVertical="true"
                	android:layout_toRightOf="@id/imageView3"
                	android:textAppearance="?android:attr/textAppearanceMedium" />

            	<LinearLayout
                    android:id="@+id/setting_linearLayout3"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="right"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/setting_pushLabel"
                    android:layout_weight="1" >

            		<ToggleButton
            		    android:id="@+id/push_toggleButton"
            		    android:layout_width="wrap_content"
            		    android:layout_height="wrap_content"
            		    android:textOn=""
                		android:textOff=""
                		android:layout_gravity="center"
                		android:layout_marginRight="15dp"
                		android:background="@drawable/custom_toggle_on" />


            	</LinearLayout>
            	
            </RelativeLayout>
            
        </TableRow>

    </TableLayout>
        
        
    <TextView
        android:id="@+id/SettingTitle4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로그램 정보"
        android:textColor="#000000"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
    <TableLayout
        android:id="@+id/setting_sectionLayout4"
        android:layout_width="match_parent"
        android:layout_height="112dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_gravity="center"
        android:background="@drawable/rounded_back" >


        <TableRow
            android:id="@+id/setting_tablerow4"
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_height="50dp" >

            <RelativeLayout
        		android:layout_width="wrap_content"
        		android:layout_height="50dp"
        		android:layout_marginTop="5dp"
        		android:background="#00000000"
        		android:layout_gravity="center" >
            
            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="40dp"
                android:layout_height="40dp"
            	android:layout_marginTop="8dp"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:src="@drawable/info" />
            
            <TextView
                android:id="@+id/setting_verLabel"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
            	android:layout_marginTop="8dp"
                android:layout_marginLeft="15dp"
                android:text="버젼정보"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/imageView4"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="8dp"
                android:layout_marginRight="10dp"
                android:text="Ver 1.0"
                android:gravity="right|center"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/setting_verLabel"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />
            
            </RelativeLayout>
            
        </TableRow>
        
            <View
            android:id="@+id/nanumsun"
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginTop="3dp"
            android:background="#AAAAAA" />

        <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#00000000"
        android:layout_gravity="center" >
        
        <Button
            android:id="@+id/setting5Button"
            android:layout_width="match_parent"
        	android:layout_height="50dp"
            android:background="#00000000"
            android:text="" />
            
        <TableRow
            android:id="@+id/setting_tablerow5"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:layout_height="50dp" >
            
            <RelativeLayout
        		android:id="@+id/settings_relativeLayout5"
        		android:layout_width="match_parent"
        		android:layout_height="50dp"
        		android:layout_gravity="center"
        		android:gravity="center" >
            
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:src="@drawable/guarantee" />
            
            <TextView
                android:id="@+id/setting_toaLabel"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="15dp"
                android:text="이용약관"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/imageView5"
                android:textAppearance="?android:attr/textAppearanceMedium" />
            
            <LinearLayout
                    android:id="@+id/setting_linearLayout5"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_toRightOf="@id/setting_toaLabel"
                    android:gravity="right"
                    android:layout_weight="1" >
            
            <ImageView
                android:id="@+id/arrowimageView3"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:src="@drawable/arrow_right" />
            
            </LinearLayout>
            
            </RelativeLayout>
            
        </TableRow>
        
        </RelativeLayout>

    </TableLayout>
            
            
        </LinearLayout>
    </ScrollView>

    
        